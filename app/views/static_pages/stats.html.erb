<div class="intro-header">
    <div class="container">

        <div class="row">
            <div class="col-lg-12">
                <div class="intro-message">
                    <h1>2017 Christmas Challenge</h1>
                    <h3>Klesis Frosh and Senior Zone</h3>
                    <h4 class="intro-count"><i>"And let us consider how to stir up one another to love and good works." Hebrews 10:24</i></h4>
                    <hr class="intro-divider">
                    <ul class="list-inline intro-social-buttons">
                        <h3 class="intro-count">
                            <%= @winner[0] %>:
                            <%= @winner[1] %>
                        </h3>

                        <% @points.each do |team| %>
                            <h4 class="intro-count">
                                <%= team[0] %>: 
                                <%= team[1] %>
                            </h4>   

                        <% end %>
                    </ul>
                </div>
            </div>
        </div>

    </div>
    <!-- /.container -->

</div>


