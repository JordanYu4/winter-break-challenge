<div class="content-section-b">
    <div class="container">
        <div class="row">
            <div class ="col-lg-4">
                <hr class="section-heading-spacer">
                <div class="clearfix"></div>
                <h2 class="section-heading">Stats</h2>

            </div>
        </div>
        <div class="row">
            <div class="col-lg-12">
                <ul class="list-group">
                        <h3 class="intro-count">
                            <%= @winner[0] %>:
                            <%= @winner[1] %>
                        </h3>

                        <% @points.each do |team| %>
                            <h4 class="intro-count">
                                <%= team[0] %>: 
                                <%= team[1] %>
                            </h4>   

                        <% end %>
                    </ul>
            </div>
        </div>
    </div>
</div>


